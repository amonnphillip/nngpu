<UserControl x:Class="nngpuVisualization.controls.NnOutput"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             xmlns:local="clr-namespace:nngpuVisualization.controls"
             mc:Ignorable="d" 
             d:DesignHeight="100" d:DesignWidth="300">
    <Grid Height="100" Width="300" Background="Coral">
        <Line Width="1" Height="100" Fill="Black" Margin="0,0,299,0"></Line>
        <Line Height="1" Fill="Black"></Line>
        
        <StackPanel x:Name="BarContainer" VerticalAlignment="Center" Width="100" Height="100" HorizontalAlignment="Left" Orientation="Horizontal">
        </StackPanel>

    </Grid>
</UserControl>
